{
  "defaultProgram": "gpm",
  "remoteInterface": {
    "address": "::1",
    "port": 55667
  },
  "joystick": {
    "driver": "linux",
    "device": "/dev/input/js1"
  },
  "mouse": {
    "driver": "linux",
    "device": "/dev/input/mouse0"
  },
  "ptu": {
    "driver": "ptu-d48e-uart",
    "device": "/dev/tty.usbserial0",
    "baudRate": 9600
  },
  "imu": {
    "driver": "mpu-6050-i2c",
    "device": "/dev/i2c-1",
    "address": 104
  },
  "gps": {
    "driver": "adafruit-ultimate-uart",
    "device": "/dev/serial0",
    "baudRate": 9600
  }
}
